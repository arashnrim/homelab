{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:best-practices"],
  "labels": ["upgrade"],
  "assignees": ["arashnrim"],
  "prFooter": null,
  "prHourlyLimit": 3,
  "ignorePaths": ["compose/archived"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pinDigest"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ]
}
